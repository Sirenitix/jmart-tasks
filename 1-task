    public List<Integer> squareOrSquareRoot (List<Integer> array){
        List<Integer> result = new ArrayList<>();
        
        for (Integer i: array) {
            if(Math.sqrt(i) % 1 == 0){
                result.add((int) Math.sqrt(i));
            }else {
                result.add((int) Math.pow(i, 2));
            }
        }
        return result;
    }
